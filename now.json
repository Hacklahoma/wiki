{
    "name": "wiki-frontend",
    "build": {
        "env": {
            "SLACK_CLIENT_ID": "@slack_client_id",
            "SLACK_CLIENT_SECRET": "@slack_client_secret",
            "TEAM_ID": "@team_id",
            "POSTGRES_URI": "@postgres_uri"
        }
    },
    "version": 2,
    "builds": [
        {
            "src": "now.js",
            "use": "@now/node",
            "config": {
                "includeFiles": ["dist/**", "src/**"]
            }
        }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/now.js" }]
}
